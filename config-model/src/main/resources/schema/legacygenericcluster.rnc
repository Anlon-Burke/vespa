# Generic, application-specific service cluster
# 
include "legacygenericmodule.rnc"

LegacyGenericCluster = element cluster {
  attribute name { text } &
  attribute command { text } &
  attribute hostservice { text }? &
  attribute num-hosts { text }? &
  GenericConfig* &
  LegacyGenericModule* &
  element node {
    service.attlist &
    attribute name { text }? &
    LegacyGenericModule* &
    GenericConfig*
  }*
}

